    </notDefaultUseAlwaysAttributes>
    <main>true</main>
    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>ВЫБРАТЬ
	ВЫБОР
		КОГДА НомераГТД.Ссылка ЕСТЬ NULL 
				ИЛИ НомераГТД.Ссылка = ЗНАЧЕНИЕ(Справочник.НомераГТД.ПустаяСсылка)
			ТОГДА &quot;&lt;Без источника происхождения>&quot;
		ИНАЧЕ НомераГТД.Наименование
	КОНЕЦ КАК Наименование,
	СУММА(ЕСТЬNULL(ТоварыОрганизацийОстатки.КоличествоОстаток, 0)) КАК Остаток,
	НомераГТД.ПризнакПроисхождения,
	НомераГТД.КодТНВЭД,
	НомераГТД.НаименованиеТовара,
	ВЫБОР
		КОГДА НомераГТД.Ссылка ЕСТЬ NULL 
			ТОГДА 1
		ИНАЧЕ 2
	КОНЕЦ КАК Приоритет,
	НомераГТД.Ссылка,
	НомераГТД.Код КАК НомерГТД,
	ВЫРАЗИТЬ(НомераГТД.Комментарий КАК СТРОКА(256)) КАК Комментарий
ИЗ
	Справочник.НомераГТД КАК НомераГТД
		ПОЛНОЕ СОЕДИНЕНИЕ РегистрНакопления.ТоварыОрганизацийБУ.Остатки(&amp;Период, &amp;УсловиеНаИзмерения) КАК ТоварыОрганизацийОстатки
		ПО (ТоварыОрганизацийОстатки.НомерГТД = НомераГТД.Ссылка)
ГДЕ
	ВЫБОР
			КОГДА &amp;ПоказыватьТолькоОстатки
					И &amp;РежимОстатков
				ТОГДА ЕСТЬNULL(ТоварыОрганизацийОстатки.КоличествоОстаток, 0) &lt;> 0
			ИНАЧЕ ВЫБОР
					КОГДА &amp;РежимОстатков
						ТОГДА ИСТИНА
					ИНАЧЕ НЕ НомераГТД.Ссылка = ЗНАЧЕНИЕ(Справочник.НомераГТД.ПустаяСсылка)
				КОНЕЦ
		КОНЕЦ

СГРУППИРОВАТЬ ПО
	НомераГТД.Ссылка,
	ВЫБОР
		КОГДА НомераГТД.Ссылка ЕСТЬ NULL 
				ИЛИ НомераГТД.Ссылка = ЗНАЧЕНИЕ(Справочник.НомераГТД.ПустаяСсылка)
			ТОГДА &quot;&lt;Без источника происхождения>&quot;
		ИНАЧЕ НомераГТД.Наименование
	КОНЕЦ,
	НомераГТД.ПризнакПроисхождения,
	НомераГТД.КодТНВЭД,
	НомераГТД.НаименованиеТовара,
	ВЫБОР
		КОГДА НомераГТД.Ссылка ЕСТЬ NULL 
			ТОГДА 1
		ИНАЧЕ 2
	КОНЕЦ</queryText>
      <mainTable>Catalog.НомераГТД</mainTable>
